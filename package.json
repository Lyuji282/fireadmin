{
  "name": "fireadmin",
  "version": "0.8.2",
  "description": "Application for Managing Firebase Applications. Includes support for multiple environments and data migrations.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "deploy": "firebase-ci deploy",
    "build:functions": "npm run build --prefix functions",
    "build:config": "firebase-ci createConfig",
    "build:testConfig": "cypress-firebase createTestEnvFile",
    "build": "react-app-rewired build",
    "eject": "react-app-rewired eject",
    "start": "react-app-rewired start",
    "start:dist": "npm run build && firebase serve --only hosting -p 3000",
    "functions:start": "npm run start --prefix functions",
    "functions:build": "npm run build --prefix functions",
    "functions:watch": "npm run watch --prefix functions",
    "functions:test": "npm run test --prefix functions",
    "lint": "eslint src functions/src functions/test test/e2e build server project.config.js",
    "lint:fix": "npm run lint -- --fix",
    "test:ui": "npm run build:testConfig && cypress run",
    "test:ui:open": "npm run build:testConfig && cypress open",
    "test:ui:stage": "npm run test:ui -- --env envName=stage",
    "test:ui:open:stage": "npm run test:ui:open -- --env envName=stage",
    "format": "prettier --single-quote --no-semi --trailing-comma none --write \"src/**/*.js\"",
    "pre-push": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prescottprue/fireadmin.git"
  },
  "engines": {
    "node": ">=8.11.0"
  },
  "author": "prescottprue (https://github.com/prescottprue)",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "3.6.0",
    "@material-ui/icons": "3.0.1",
    "classnames": "^2.2.6",
    "codemirror": "^5.32.0",
    "date-fns": "^1.29.0",
    "firebase": "^5.0.3",
    "firepad": "^1.4.0",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.1",
    "react": "^16.6.3",
    "react-dnd": "^2.5.4",
    "react-dnd-html5-backend": "^2.5.4",
    "react-dom": "^16.6.3",
    "react-dropzone": "^4.2.1",
    "react-ga": "^2.3.5",
    "react-google-button": "^0.3.0",
    "react-instantsearch": "^4.2.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-redux-firebase": "^2.1.6",
    "react-router": "^3.0.0",
    "recompose": "^0.30.0",
    "redbox-react": "^1.3.6",
    "redux": "^4.0.0",
    "redux-auth-wrapper": "^1.0.0",
    "redux-firestore": "^0.5.7",
    "redux-form": "^7.4.0",
    "redux-form-material-ui": "^5.0.0-beta.3",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "babel-eslint": "^10.0.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "cross-env": "^5.2.0",
    "customize-cra": "^0.2.7",
    "cypress": "^3.1.2",
    "cypress-firebase": "^0.0.8",
    "eslint": "^5.6.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-standard": "^3.0.1",
    "firebase-tools": "^6.1.0",
    "fs-extra": "^7.0.1",
    "husky": "^1.2.0",
    "node-sass": "^4.10.0",
    "prettier": "^1.14.0",
    "react-app-rewired": "^1.6.2",
    "react-scripts": "2.1.1",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.6.1",
    "yargs": "^12.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
